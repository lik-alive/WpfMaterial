<Window x:Class="WpfMaterial.Preview.NumericUpDownStylePrev"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:cel="clr-namespace:WpfControls;assembly=WpfControls"
        x:Name="numericUpDownStylePrev"
        Title="NumericUpDownStylePrev" Height="300" Width="300">
    <Grid Background="White">
        <cel:NumericUpDown x:Name="num1" Value="{Binding ElementName=numericUpDownStylePrev, Path=Age}" Increment="0.1" Minimum="10" Maximum="100" Width="100" Height="22">
            
        </cel:NumericUpDown>
        <cel:NumericUpDown Value="1" Width="100" Height="22" Margin="0,70,0,0" IsReadOnly="True"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="121,218,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="96,46,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>

</Window>
