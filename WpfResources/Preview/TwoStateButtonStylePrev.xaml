<Window x:Class="WpfResources.Preview.TwoStateButtonStylePrev"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:cel="clr-namespace:WpfControls;assembly=WpfControls"
        Title="TwoStateButtonStylePrev" Height="300" Width="300">
    <Grid Background="#DDDDDD">
        <StackPanel SnapsToDevicePixels="True" ClipToBounds="True">
            <cel:TwoStateButton x:Name="button" Width="40" Height="40" IconURI="/WpfResources;component/Preview/Icons/icon1.ico" IsSelected="False" Click="TwoStateButton_Click_1"/>
            <cel:TwoStateButton x:Name="sourceButton" Width="40" Height="40" IsSelected="False"/>
        </StackPanel>
        
    </Grid>
</Window>
